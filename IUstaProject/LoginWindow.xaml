<Window x:Class="IUstaProject.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IUstaProject"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
            <TextBox x:Name="Email" Height="50" Width="400" Margin="65"></TextBox>
            <TextBox x:Name="Pasword" Height="50" Width="400" ></TextBox>
            <Button Content="" Height="50" Width="150" Margin="25"></Button>
        </StackPanel>
    </Grid>
</Window>
